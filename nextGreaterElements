class Solution {
public:
    vector<int> nextGreaterElements(vector<int>& nums) 
    {
       int n=nums.size();
       stack<int>s;
       vector<int>ans(n,-1);

       for(int i=2*n-1;i>=0;i--)
       {
        int idx=i%n;
        while(!s.empty()&&s.top()<=nums[idx])
        {
            s.pop();
        }
        if(!s.empty())
        {
            ans[idx]=s.top();
        }
        s.push(nums[idx]);

       }
        return ans;
    }
};
